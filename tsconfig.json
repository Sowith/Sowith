{
	"compilerOptions": {
		"module": "ES6",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"baseUrl": "./src",
		"paths": {
			"react-router-dom": ["../node_modules/react-router-dom"],
			"*": ["*", "src/*"]
		},
		"jsx": "react-jsx",
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noImplicitAny": false,
		"types": ["kakao.maps.d.ts"]
	},
	"include": ["src"],
	"typeRoots": ["src/types"]
}
